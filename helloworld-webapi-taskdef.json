{
    "family": "helloworld--containers",
    "containerDefinitions": [
      {
        "name": "helloworld",
        "image": "196093915263.dkr.ecr.ap-southeast-2.amazonaws.com/helloworld-webapi:latest",
        "cpu": 2,
        "memory": 300,
        "portMappings": [
            {
                "containerPort": 80,
                "hostPort": 80
            }
        ],
        "essential": true,
        "environment": [
            {
                "name": "ENV",
                "value": "production"
            }
        ],
        "requiresCompatibilities": [
            "EC2", "FARGATE"
        ]
      }
    ]
  }