{
    "family": "helloworld--containers",
    "networkMode": "awsvpc",
    "cpu": 2,
    "containerDefinitions": [
      {
        "name": "helloworld",
        "image": "196093915263.dkr.ecr.ap-southeast-2.amazonaws.com/helloworld-webapi:latest",
        
        "memory": 512,
        "portMappings": [
            {
                "containerPort": 80,
                "hostPort": 80
            }
        ],
        "essential": true,
        "environment": [
            {
                "name": "ENV",
                "value": "production"
            }
        ]
      }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ]
  }